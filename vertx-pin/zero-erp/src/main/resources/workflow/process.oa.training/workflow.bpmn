      <bpmn:incoming>t.begin-running</bpmn:incoming>
      <bpmn:outgoing>t.running-d</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="e.running.d">
      <bpmn:incoming>t.running-d</bpmn:incoming>
      <bpmn:outgoing>t.running-feedback</bpmn:outgoing>
      <bpmn:outgoing>t.running-running</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="t.running-d" sourceRef="e.running" targetRef="e.running.d" />
    <bpmn:sequenceFlow id="t.running-feedback" name="上传完成" sourceRef="e.running.d" targetRef="e.summary">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{confirmed==true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:userTask id="e.summary" name="培训总结报告" camunda:formKey="camunda-forms:deployment:training.summary.form">
      <bpmn:incoming>t.running-feedback</bpmn:incoming>
      <bpmn:outgoing>t.summary-end</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:endEvent id="e.end" name="培训结束">
      <bpmn:incoming>t.summary-end</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="t.summary-end" sourceRef="e.summary" targetRef="e.end" />
    <bpmn:sequenceFlow id="t.running-running" name="答卷未收齐" sourceRef="e.running.d" targetRef="e.running">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{confirmed==false}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="t.approve-boss" name="通过" sourceRef="e.approve.d" targetRef="e.boss" />
    <bpmn:exclusiveGateway id="e.approve.boss.d">
      <bpmn:incoming>t.boss-d</bpmn:incoming>
      <bpmn:outgoing>t.boss-draft</bpmn:outgoing>
      <bpmn:outgoing>t.boss-begin</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="t.boss-d" sourceRef="e.boss" targetRef="e.approve.boss.d" />
    <bpmn:sequenceFlow id="t.boss-draft" name="领导审批未通过" sourceRef="e.approve.boss.d" targetRef="e.draft">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{confirmed==false}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="t.boss-begin" name="领导审批通过" sourceRef="e.approve.boss.d" targetRef="e.begin">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{confirmed==true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:userTask id="e.begin" name="填写培训信息&#10;资料上传" camunda:formKey="camunda-forms:deployment:training.upload.form">
      <bpmn:incoming>t.boss-begin</bpmn:incoming>
      <bpmn:outgoing>t.begin-running</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="t.begin-running" sourceRef="e.begin" targetRef="e.running" />
    <bpmn:userTask id="e.boss" name="领导审批&#10;培训申请" camunda:formKey="camunda-forms:deployment:training.boss.form">
      <bpmn:incoming>t.approve-boss</bpmn:incoming>
      <bpmn:outgoing>t.boss-d</bpmn:outgoing>
    </bpmn:userTask>
