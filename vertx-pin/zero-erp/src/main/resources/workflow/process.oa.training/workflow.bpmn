    </bpmn:endEvent>
    <bpmn:sequenceFlow id="t.summary-end" sourceRef="e.summary" targetRef="e.end" />
    <bpmn:sequenceFlow id="t.running-running" name="答卷未收齐" sourceRef="e.running.d" targetRef="e.running">
