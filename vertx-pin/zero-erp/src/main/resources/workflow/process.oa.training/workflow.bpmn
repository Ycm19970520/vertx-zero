    <bpmn:process id="process.oa.training" name="培训管理流程" isExecutable="true">
        <bpmn:startEvent id="e.start" name="发起培训申请" camunda:formKey="camunda-forms:deployment:training.start.form">
            <bpmn:outgoing>t.start-d</bpmn:outgoing>
        </bpmn:startEvent>
        <bpmn:exclusiveGateway id="e.start.d">
            <bpmn:incoming>t.start-d</bpmn:incoming>
            <bpmn:outgoing>t.start-approve</bpmn:outgoing>
            <bpmn:outgoing>t.start-draft</bpmn:outgoing>
        </bpmn:exclusiveGateway>
        <bpmn:sequenceFlow id="t.start-d" sourceRef="e.start" targetRef="e.start.d"/>
        <bpmn:sequenceFlow id="t.start-approve" name="直接申请" sourceRef="e.start.d" targetRef="e.approve">
            <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{draft==false}</bpmn:conditionExpression>
        </bpmn:sequenceFlow>
        <bpmn:userTask id="e.approve" name="培训审批" camunda:formKey="camunda-forms:deployment:training.approval.form">
            <bpmn:incoming>t.start-approve</bpmn:incoming>
            <bpmn:incoming>t.draft-approve</bpmn:incoming>
            <bpmn:outgoing>t.approve-d</bpmn:outgoing>
        </bpmn:userTask>
        <bpmn:sequenceFlow id="t.start-draft" name="存草稿" sourceRef="e.start.d" targetRef="e.draft">
            <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{draft==true}</bpmn:conditionExpression>
        </bpmn:sequenceFlow>
        <bpmn:sequenceFlow id="t.draft-approve" sourceRef="e.draft" targetRef="e.approve"/>
        <bpmn:userTask id="e.draft" name="培训申请" camunda:formKey="camunda-forms:deployment:training.draft.form">
            <bpmn:incoming>t.start-draft</bpmn:incoming>
            <bpmn:incoming>t.approve-draft</bpmn:incoming>
            <bpmn:outgoing>t.draft-approve</bpmn:outgoing>
        </bpmn:userTask>
        <bpmn:exclusiveGateway id="e.approve.d" name="审批是否通过？">
            <bpmn:incoming>t.approve-d</bpmn:incoming>
            <bpmn:outgoing>t.approve-draft</bpmn:outgoing>
            <bpmn:outgoing>t.approve-running</bpmn:outgoing>
        </bpmn:exclusiveGateway>
        <bpmn:sequenceFlow id="t.approve-d" sourceRef="e.approve" targetRef="e.approve.d"/>
        <bpmn:sequenceFlow id="t.approve-draft" name="审批未通过" sourceRef="e.approve.d" targetRef="e.draft">
            <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{confirmed==false}</bpmn:conditionExpression>
        </bpmn:sequenceFlow>
        <bpmn:sequenceFlow id="t.approve-running" name="通过" sourceRef="e.approve.d" targetRef="e.running">
            <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{confirmed==true}</bpmn:conditionExpression>
        </bpmn:sequenceFlow>
        <bpmn:userTask id="e.running" name="培训实施&#10;（信息记录）"
                       camunda:formKey="camunda-forms:deployment:training.running.form">
            <bpmn:incoming>t.approve-running</bpmn:incoming>
            <bpmn:incoming>t.running-running</bpmn:incoming>
            <bpmn:outgoing>t.running-d</bpmn:outgoing>
        </bpmn:userTask>
        <bpmn:exclusiveGateway id="e.running.d" name="是否完成？">
            <bpmn:incoming>t.running-d</bpmn:incoming>
            <bpmn:outgoing>t.running-feedback</bpmn:outgoing>
            <bpmn:outgoing>t.running-running</bpmn:outgoing>
        </bpmn:exclusiveGateway>
        <bpmn:sequenceFlow id="t.running-d" sourceRef="e.running" targetRef="e.running.d"/>
        <bpmn:sequenceFlow id="t.running-feedback" name="培训完成" sourceRef="e.running.d" targetRef="e.feedback">
            <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{confirmed==true}</bpmn:conditionExpression>
        </bpmn:sequenceFlow>
        <bpmn:userTask id="e.feedback" name="培训效果评价" camunda:formKey="camunda-forms:deployment:training.feedback.form">
            <bpmn:incoming>t.running-feedback</bpmn:incoming>
            <bpmn:outgoing>t.feedback-summary</bpmn:outgoing>
        </bpmn:userTask>
        <bpmn:userTask id="e.summary" name="培训效果总结" camunda:formKey="camunda-forms:deployment:training.summary.form">
            <bpmn:incoming>t.feedback-summary</bpmn:incoming>
            <bpmn:outgoing>t.summary-end</bpmn:outgoing>
        </bpmn:userTask>
        <bpmn:sequenceFlow id="t.feedback-summary" sourceRef="e.feedback" targetRef="e.summary"/>
        <bpmn:endEvent id="e.end" name="培训结束">
            <bpmn:incoming>t.summary-end</bpmn:incoming>
        </bpmn:endEvent>
        <bpmn:sequenceFlow id="t.summary-end" sourceRef="e.summary" targetRef="e.end"/>
        <bpmn:sequenceFlow id="t.running-running" name="培训未完成" sourceRef="e.running.d" targetRef="e.running">
            <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{confirmed==false}</bpmn:conditionExpression>
        </bpmn:sequenceFlow>
    </bpmn:process>
