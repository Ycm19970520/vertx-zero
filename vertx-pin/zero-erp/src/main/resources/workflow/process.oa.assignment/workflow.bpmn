  <bpmn:process id="process.oa.assignment" name="工作交接流程" isExecutable="true">
    <bpmn:startEvent id="e.start" name="发起交接工作申请" camunda:formKey="camunda-forms:deployment:assignment.start.form">
      <bpmn:outgoing>t.start-d</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:exclusiveGateway id="e.start.d">
      <bpmn:incoming>t.start-d</bpmn:incoming>
      <bpmn:outgoing>t.start-evaluate</bpmn:outgoing>
      <bpmn:outgoing>t.start-draft</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="t.start-d" sourceRef="e.start" targetRef="e.start.d" />
    <bpmn:sequenceFlow id="t.start-evaluate" name="提交" sourceRef="e.start.d" targetRef="e.evaluate">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{draft==false}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:userTask id="e.evaluate" name="工作交接&#10;内容确认" camunda:formKey="camunda-forms:deployment:assignment.evaluate.form">
      <bpmn:incoming>t.start-evaluate</bpmn:incoming>
      <bpmn:incoming>t.draft-evaluate</bpmn:incoming>
      <bpmn:outgoing>t.evaluate-d</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="t.start-draft" name="暂存" sourceRef="e.start.d" targetRef="e.draft">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{draft==true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="t.draft-evaluate" name="提交申请单" sourceRef="e.draft" targetRef="e.evaluate" />
    <bpmn:userTask id="e.draft" name="工作交接申请&#10;暂存编辑" camunda:formKey="camunda-forms:deployment:assignment.draft.form">
      <bpmn:incoming>t.start-draft</bpmn:incoming>
      <bpmn:incoming>t.evaluate-draft</bpmn:incoming>
      <bpmn:incoming>t.approve-draft</bpmn:incoming>
      <bpmn:outgoing>t.draft-evaluate</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="e.evaluate.d">
      <bpmn:incoming>t.evaluate-d</bpmn:incoming>
      <bpmn:outgoing>t.evaluate-draft</bpmn:outgoing>
      <bpmn:outgoing>t.evaluate-approve</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="t.evaluate-d" sourceRef="e.evaluate" targetRef="e.evaluate.d" />
    <bpmn:sequenceFlow id="t.evaluate-draft" name="工作内容&#10;确认未通过" sourceRef="e.evaluate.d" targetRef="e.draft">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{confirmed==false}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="t.evaluate-approve" name="确认" sourceRef="e.evaluate.d" targetRef="e.approve">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{confirmed==true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:userTask id="e.approve" name="领导审批&#10;工作交接单" camunda:formKey="camunda-forms:deployment:assignment.approval.form">
      <bpmn:incoming>t.evaluate-approve</bpmn:incoming>
      <bpmn:outgoing>t.approve-d</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:endEvent id="e.end" name="完成工作交接申请">
      <bpmn:incoming>t.approve-end</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="t.approve-end" name="通过" sourceRef="e.approve.d" targetRef="e.end">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{confirmed==true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:exclusiveGateway id="e.approve.d">
      <bpmn:incoming>t.approve-d</bpmn:incoming>
      <bpmn:outgoing>t.approve-end</bpmn:outgoing>
      <bpmn:outgoing>t.approve-draft</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="t.approve-d" sourceRef="e.approve" targetRef="e.approve.d" />
    <bpmn:sequenceFlow id="t.approve-draft" name="领导审批&#10;未通过" sourceRef="e.approve.d" targetRef="e.draft">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{confirmed==false}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
  </bpmn:process>
