  <bpmn:process id="process.vendor.hour" name="工时补签流程" isExecutable="true">
    <bpmn:startEvent id="e.start" name="新建工时&#10;补签记录单" camunda:formKey="camunda-forms:deployment:hour.start.form">
      <bpmn:outgoing>t.start-d</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:exclusiveGateway id="e.start.d">
      <bpmn:incoming>t.start-d</bpmn:incoming>
      <bpmn:outgoing>t.start-draft</bpmn:outgoing>
      <bpmn:outgoing>t.start-approve</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="t.start-d" sourceRef="e.start" targetRef="e.start.d" />
    <bpmn:userTask id="e.approve" name="项目经理&#10;审批" camunda:formKey="camunda-forms:deployment:hour.approval.form">
      <bpmn:incoming>t.draft-approve</bpmn:incoming>
      <bpmn:incoming>t.start-approve</bpmn:incoming>
      <bpmn:outgoing>t.approve-d</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="t.start-draft" name="暂存" sourceRef="e.start.d" targetRef="e.draft">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{draft==true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="t.draft-approve" name="提交记录单" sourceRef="e.draft" targetRef="e.approve" />
    <bpmn:userTask id="e.draft" name="工时补签记录单&#10;暂存编辑" camunda:formKey="camunda-forms:deployment:hour.draft.form">
      <bpmn:incoming>t.start-draft</bpmn:incoming>
      <bpmn:incoming>t.approve-draft</bpmn:incoming>
      <bpmn:incoming>t.dept-draft</bpmn:incoming>
      <bpmn:outgoing>t.draft-approve</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="e.approve.d">
      <bpmn:incoming>t.approve-d</bpmn:incoming>
      <bpmn:outgoing>t.approve-draft</bpmn:outgoing>
      <bpmn:outgoing>t.approve-dept</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="t.approve-d" sourceRef="e.approve" targetRef="e.approve.d" />
    <bpmn:sequenceFlow id="t.approve-draft" name="项目经理&#10;审批未通过" sourceRef="e.approve.d" targetRef="e.draft">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{confirmed==false}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:endEvent id="e.end" name="补签完成">
      <bpmn:incoming>t.close-end</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="t.start-approve" name="提交" sourceRef="e.start.d" targetRef="e.approve">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{draft==false}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:userTask id="e.dept" name="使用部门&#10;审批" camunda:formKey="camunda-forms:deployment:hour.dept.form">
      <bpmn:incoming>t.approve-dept</bpmn:incoming>
      <bpmn:outgoing>t.dept-d</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="e.dept.d">
      <bpmn:incoming>t.dept-d</bpmn:incoming>
      <bpmn:outgoing>t.dept-draft</bpmn:outgoing>
      <bpmn:outgoing>t.dept-close</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="t.dept-d" sourceRef="e.dept" targetRef="e.dept.d" />
    <bpmn:sequenceFlow id="t.dept-draft" name="使用部门&#10;审批未通过" sourceRef="e.dept.d" targetRef="e.draft">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{confirmed==false}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="t.dept-close" name="通过" sourceRef="e.dept.d" targetRef="e.close">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{confirmed==true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="t.close-end" sourceRef="e.close" targetRef="e.end" />
    <bpmn:userTask id="e.close" name="补签人确认&#10;并关闭流程" camunda:formKey="camunda-forms:deployment:hour.close.form">
      <bpmn:incoming>t.dept-close</bpmn:incoming>
      <bpmn:outgoing>t.close-end</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="t.approve-dept" name="通过" sourceRef="e.approve.d" targetRef="e.dept">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{confirmed==true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
  </bpmn:process>
