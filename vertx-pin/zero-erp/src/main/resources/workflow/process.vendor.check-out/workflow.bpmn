      <bpmn:incoming>t.approve-draft</bpmn:incoming>
      <bpmn:outgoing>t.draft-approve</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="e.approve.d" name="审批通过？">
      <bpmn:incoming>t.approve-d</bpmn:incoming>
      <bpmn:outgoing>t.approve-draft</bpmn:outgoing>
      <bpmn:outgoing>t.approve-verify</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="t.approve-d" sourceRef="e.approve" targetRef="e.approve.d" />
    <bpmn:sequenceFlow id="t.approve-draft" name="不通过" sourceRef="e.approve.d" targetRef="e.draft">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{confirmed==false}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="t.approve-verify" name="通过" sourceRef="e.approve.d" targetRef="e.logout">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{confirmed==true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:userTask id="e.logout" name="账号注销" camunda:formKey="camunda-forms:deployment:check-out.logout.form">
      <bpmn:incoming>t.approve-verify</bpmn:incoming>
      <bpmn:outgoing>t.verify-end</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:endEvent id="e.end" name="离场申请完成">
