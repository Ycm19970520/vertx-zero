    </bpmn:userTask>
    <bpmn:exclusiveGateway id="e.boss.d">
      <bpmn:incoming>t.boss-d</bpmn:incoming>
      <bpmn:outgoing>t.boss-init</bpmn:outgoing>
      <bpmn:outgoing>t.boss-draft</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="t.boss-d" sourceRef="e.boss" targetRef="e.boss.d" />
    <bpmn:sequenceFlow id="t.boss-init" name="通过" sourceRef="e.boss.d" targetRef="e.init">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{confirmed==true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="t.boss-draft" name="领导审批&#10;未通过" sourceRef="e.boss.d" targetRef="e.draft">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{confirmed==false}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
