      <bpmn:incoming>t.draft-approve</bpmn:incoming>
      <bpmn:incoming>t.start-approve</bpmn:incoming>
      <bpmn:outgoing>t.vendor-d</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="t.start-draft" name="暂存" sourceRef="e.start.d" targetRef="e.draft">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{draft==true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="t.draft-approve" name="提交考核记录单" sourceRef="e.draft" targetRef="e.vendor" />
    <bpmn:userTask id="e.draft" name="考核内容&#10;暂存编辑" camunda:formKey="camunda-forms:deployment:assessment.draft.form">
      <bpmn:incoming>t.start-draft</bpmn:incoming>
      <bpmn:incoming>t.manager-draft</bpmn:incoming>
      <bpmn:incoming>t.vendor-draft</bpmn:incoming>
      <bpmn:outgoing>t.draft-approve</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="e.manager.d">
      <bpmn:incoming>t.manager-d</bpmn:incoming>
      <bpmn:outgoing>t.manager-draft</bpmn:outgoing>
      <bpmn:outgoing>t.manager-adjust</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="t.manager-draft" name="项目经理确认&#10;未同意" sourceRef="e.manager.d" targetRef="e.draft">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{confirmed==false}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:endEvent id="e.end" name="考核完成">
      <bpmn:incoming>t.adjust-end</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:userTask id="e.manager" name="项目经理&#10;确认考核结果" camunda:formKey="camunda-forms:deployment:assessment.manager.form">
      <bpmn:incoming>t.vendor-manager</bpmn:incoming>
      <bpmn:outgoing>t.manager-d</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="t.start-approve" name="提交" sourceRef="e.start.d" targetRef="e.vendor">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{draft==false}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:exclusiveGateway id="e.vendor.d">
      <bpmn:incoming>t.vendor-d</bpmn:incoming>
      <bpmn:outgoing>t.vendor-manager</bpmn:outgoing>
      <bpmn:outgoing>t.vendor-draft</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="t.vendor-d" sourceRef="e.vendor" targetRef="e.vendor.d" />
    <bpmn:sequenceFlow id="t.vendor-manager" name="确认" sourceRef="e.vendor.d" targetRef="e.manager">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{confirmed==true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="t.vendor-draft" name="供应商确认&#10;未同意" sourceRef="e.vendor.d" targetRef="e.draft">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{confirmed==false}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="t.manager-d" sourceRef="e.manager" targetRef="e.manager.d" />
    <bpmn:sequenceFlow id="t.manager-adjust" name="确认" sourceRef="e.manager.d" targetRef="e.adjust">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{confirmed==true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:userTask id="e.adjust" name="整改意见确认" camunda:formKey="camunda-forms:deployment:assessment.adjust.form">
      <bpmn:incoming>t.manager-adjust</bpmn:incoming>
      <bpmn:outgoing>t.adjust-end</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="t.adjust-end" sourceRef="e.adjust" targetRef="e.end" />
  </bpmn:process>
