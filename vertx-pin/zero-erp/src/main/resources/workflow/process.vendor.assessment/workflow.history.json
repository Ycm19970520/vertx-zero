{
    "hidden": [
        "assessOn"
    ],
    "segment": {
        "TICKET": [
            [
                {
                    "metadata": "startAt,入场时间,8,,aiDatePicker",
                    "optionJsx.format": "YYYY-MM-DD",
                    "optionJsx.disabledDate": "propFromNow",
                    "optionJsx.depend.impact": {
                        "duration": {
                            "input": {
                                "start": "VALUE",
                                "end": "FORM:endAt"
                            },
                            "mode": "day",
                            "target": "days"
                        }
                    },
                    "optionConfig.rules": [
                        "required,请填写入场时间！"
                    ],
                    "moment": true
                },
                {
                    "metadata": "endAt,离场时间,8,,aiDatePicker",
                    "optionJsx.format": "YYYY-MM-DD",
                    "optionJsx.disabledDate": "propFromNow",
                    "optionJsx.depend.impact": {
                        "duration": {
                            "input": {
                                "start": "FORM:startAt",
                                "end": "VALUE"
                            },
                            "mode": "day",
                            "target": "days"
                        }
                    },
                    "optionConfig.rules": [
                        "required,请填写离场时间！"
                    ],
                    "moment": true
                },
                {
                    "metadata": "days,累积天数,8,,,readOnly=true,addonAfter=天"
                }
            ],
            [
                {
                    "metadata": "assessOnName,被评价人,8,,aiListSelector,placeholder=（谁将被评价）",
                    "optionJsx.config": {
                        "ajax": {
                            "metadata": "POST,/api/user/search/employee,1,10,sorter=createdAt`ASC",
                            "params.criteria": {
                                "sigma": "PROP:app.sigma",
                                "": true
                            }
                        },
                        "linker": {
                            "key": "assessOn",
                            "realname": "assessOnName",
                            "workNumber": "assessOnNo",
                            "mobile": "assessOnMobile"
                        },
                        "table": {
                            "columns": [
                                {
                                    "title": "部门",
                                    "dataIndex": "deptId",
                                    "$render": "USER",
                                    "$config": {
                                        "uri": "/api/dept/:key",
                                        "field": "name",
                                        "icon": false
                                    },
                                    "$empty": "（无）"
                                },
                                "workNumber,员工号",
                                "username,登录账号",
                                "realname,姓名",
                                "workPhone,工作电话"
                            ]
                        },
                        "validation": "请选择处理人！",
                        "window": "选择处理人,选择,关闭,false,800,false",
                        "search": {
                            "username,c": "账号",
                            "realname,c": "姓名"
                        }
                    },
                    "optionJsx.allowClear": true,
                    "optionConfig.rules": [
                        "required,请填写被评价人！"
                    ]
                },
                "assessOnNo,被评价人工号,8,,,readOnly=true",
                "assessOnMobile,被评价人电话,8,,,readOnly=true"
            ],
            [
                {
                    "metadata": "classification,类型,8,,aiSelect",
                    "optionJsx.config.datum": "source=oa.customer.type,key=code,label=name",
                    "optionConfig.rules": [
                        "required,请选择入场类型！"
                    ]
                },
                {
                    "metadata": "assessScore,评价分数,8,,aiInputNumber,precision=0,min=1,max=100",
                    "optionJsx.depend.impact": {
                        "evaluate": {
                            "input": {
                                "value": "VALUE"
                            },
                            "range": {
                                "[0,60)": "bad",
                                "[60,75)": "common",
                                "[75,90)": "better",
                                "[90,100]": "best"
                            },
                            "target": "assessValue"
                        }
                    },
                    "optionConfig.rules": [
                        "required,请填写评价分数！"
                    ]
                },
                {
                    "metadata": "assessValue,评价结果,8,,aiSelect,readOnly=true",
                    "optionJsx.config.datum": "source=oa.customer.rank,key=code,label=name"
                }
            ],
            [
                "commentDept,部门改进建议,24,,aiTextArea,rows=2,placeholder=使用部门对评价人的改进建议。"
            ],
            [
                "commentAssess,被评价人意见,24,,aiTextArea,rows=2,placeholder=被评价人意见反馈以及相关说明。"
            ],
            [
                "commentLeader,项目经理意见,24,,aiTextArea,rows=2,placeholder=项目经理整改意见以及相关说明。"
            ]
        ],
        "END_CLOSE": "END_STANDARD"
    }
}