    "name,名称",
    {
        "metadata": "type,集成类型,DATUM,true",
        "$datum": "source=zero.integration,value=key,display=name",
        "$config": {
            "adorn": {
                "field": "code",
                "items": {
                    "ftp": "upload,14,#996",
                    "email": "mail,14,#F39802",
                    "restful": "api,14,#666",
                    "open.source": "fork,14,#c39"
                }
            }
        }
    },
    "ipV4,IP地址",
    "hostname,主机名",
    "port,端口号",
    "protocol,协议类型",
    "endpoint,端地址",
    {
        "title": "更新人",
        "dataIndex": "createdBy",
        "$render": "USER",
        "$config": {
            "uri": "/api/user/:key",
            "field": "realname"
        },
        "$empty": "（系统）"
    },
    {
        "title": "更新时间",
        "dataIndex": "createdAt",
        "$render": "DATE",
        "$format": "YYYY-MM-DD"
    }
